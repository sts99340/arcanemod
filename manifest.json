{
    "name": "Arcane",
    "description": "Based on the TV series arcane League of Legends",
    "developer":
    {
        "name": "FutureHammer"
    },
    "icons":
    {
        "512": "Arcane_Logo.png"
    },
    "manifest_version": 3,
    "mod":
    {
        "license": "license.txt",
        "payload":
        {
            "background_music":
            [
                "music/piltover_background_music.m4a"
            ],
            "browser_sounds":
            {
                "CLICK":
                [
                    "sound/headshot_sound.m4a"
                ],
                "FEATURE_SWITCH_OFF":
                [
                    "sound/feature_switch_off.mp3"
                ],
                "FEATURE_SWITCH_ON":
                [
                    "sound/feature_switch_on.mp3"
                ],
                "HOVER":
                [
                    "sound/hextech_gates.m4a"
                ],
                "HOVER_UP":
                [
                    "sound/hextech_gates.m4a"
                ],
                "IMPORTANT_CLICK":
                [
                    "sound/headshot_sound.m4a"
                ],
                "LEVEL_UPGRADE":
                [
                    "sound/level_upgrade.mp3"
                ],
                "LIMITER_OFF":
                [
                    "sound/limiter_off.mp3"
                ],
                "LIMITER_ON":
                [
                    "sound/limiter_on.mp3"
                ],
                "SWITCH_TOGGLE":
                [
                    "sound/switch.mp3"
                ],
                "TAB_CLOSE":
                [
                    "sound/warrick_chomp.m4a"
                ],
                "TAB_INSERT":
                [
                    "sound/hextech_smash.m4a"
                ],
                "TAB_SLASH":
                [
                    "sound/tab_slash.mp3"
                ]
            },
            "keyboard_sounds":
            {
                "TYPING_BACKSPACE":
                [
                    "keyboard/explosion_sound.m4a"
                ],
                "TYPING_ENTER":
                [
                    "keyboard/Dong_turrets.m4a"
                ],
                "TYPING_LETTER":
                [
                    "keyboard/gun_sound_1.wav",
                    "keyboard/gun_sound_2.wav",
                    "keyboard/gun_sound_3.wav",
                    "keyboard/gun_sound_4.wav"
                ],
                "TYPING_SPACE":
                [
                    "keyboard/vi_punch.m4a"
                ]
            },
            "page_styles":
            [
                {
                    "css":
                    [
                        "webmodding/opera.css"
                    ],
                    "matches":
                    [
                        "https://*.opera.com/*"
                    ]
                }
            ],
            "shaders":
            {
                "Matrix green":
                {
                    "path": "shader/matrix.txt"
                },
                "Wave":
                {
                    "animation":
                    {
                        "duration": 60,
                        "steps": 3600
                    },
                    "path": "shader/wave.txt"
                }
            },
            "theme":
            {
                "dark":
                {
                    "gx_accent":
                    {
                        "h": 171,
                        "s": 96,
                        "l": 55
                    },
                    "gx_secondary_base":
                    {
                        "h": 109,
                        "s": 90,
                        "l": 16
                    }
                },
                "light":
                {
                    "gx_accent":
                    {
                        "h": 52,
                        "s": 100,
                        "l": 33
                    },
                    "gx_secondary_base":
                    {
                        "h": 238,
                        "s": 54,
                        "l": 16
                    }
                }
            },
            "wallpaper":
            {
                "dark":
                {   
                    "image": "wallpaper/dark_background.jpg",
                    "text_color": "#FFFFFF",
                    "text_shadow": "#757575"
                },
                "light":
                {
                    "image": "wallpaper/light_arcane_background.jpg",
                    "text_color": "#FFFFFF",
                    "text_shadow": "#0B000E"
                }
            }
        },
        "schema_version": 1
    },
    "version": "1.5"
}
